{
    "command": "scaffold",
    "description": "scaffold repositories, mappers, models",
    "usage": "brewery scaffold --options",
    "sub-commands": {
        "subcommand1": {
            "description": "subcommand1 description",
            "usage": "brewery migrate:subcommand1 --options",
            "options": {
                "--exampleOption": {
                    "description": "example option description"
                },
                "--exampleOption2": {
                    "description": "example option description"
                },
                "--exampleOption3": {
                    "description": "example option description"
                }
            },
            "defaultOptions": ["--exampleOption", "exampleOption1"]
        }
    },
    "options": {
        "--withModels": {
            "description": "scaffold models based on database tables"
        },
        "--withRepositories": {
            "description": "scaffold repositories based on models"
        },
        "--withMappers": {
            "description": "scaffold mappers based on domain"
        }
    },
    "defaultOptions": ["--withModels", "--withRepositories", "--withMappers"],
    "execPath": "./actions/scaffold/"
}